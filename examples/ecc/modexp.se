b = msg.data[0]
e = msg.data[1]
m = msg.data[2]
if e == 0:
    return(1)
elif e == 1:
    return(b)
elif !(e%2):
    sub = call(contract.address, [b, div(e, 2), m], 3)
    return(mulmod(sub, sub, m))
else:
    sub = call(contract.address, [b, div(e, 2), m], 3)
    return(mulmod(mulmod(sub, sub, m), b, m))
